{
  "name": "dantweb/atomizer",
  "description": "A project utilizing PHPUnit and Symfony components",
  "license": "CC-BY-NC-4.0",
  "type": "project",
  "require": {
    "php": "^8.2",
    "psr/log": "^1.1 || ^2.0 || ^3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Dantweb\\Atomizer\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Dantweb\\Atomizer\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit",
    "phpunit": "vendor/bin/phpunit"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}